@using Impersonation.Interfaces.Accessors
@using Umbraco.Cms.Core.Security
@inject IBackofficeUserAccessor BackOfficeSecurity
@inject IMemberManager MemberManager


@{
    var member = await MemberManager.GetCurrentMemberAsync();
    if (BackOfficeSecurity.BackofficeUser?.IsAuthenticated == false)
    {
        return;
    }
}

@if (member != null)
{
    <umb-act-toggle></umb-act-toggle>
}
else
{
    <umb-act-not-toggle></umb-act-not-toggle>
}
